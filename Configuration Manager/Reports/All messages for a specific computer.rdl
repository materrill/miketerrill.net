    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@UserSIDs">
            <Value>=Parameters!UserSIDs.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@computername">
            <Value>=Parameters!computername.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>begin 
 if (@computername = '') 
  SELECT DISTINCT SYS.Netbios_Name0 from fn_rbac_R_System(@UserSIDs)  SYS  ORDER By SYS.Netbios_Name0 
 else 
  SELECT DISTINCT SYS.Netbios_Name0 from fn_rbac_R_System(@UserSIDs)  SYS  
  WHERE SYS.Netbios_Name0 like @computername
  ORDER By SYS.Netbios_Name0 
end</CommandText>
